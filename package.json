{
  "name": "@reiebenezer/ts-utils",
  "version": "0.1.1",
  "description": "A personal compilation of Typescript Utilities and Helper functions",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "scripts": {
    "build": "tsup src --format cjs,esm --dts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "color-convert": "^3.1.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  }
}
